{
  "@context": {
    "@vocab": "http://linkedsoftwaredependencies.org/vocabulary/components#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "npmd": "http://example.org/npmdataset/",
    "npm": "http://example.org/npm#",
    "ldfs": "npmd:bundles/npm/ldf-server/",
    "rdfa": "http://www.w3.org/ns/rdfa#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "value": { "@id": "rdf:value", "@type": "@id"}
  },
  "@id": "npmd:bundles/npm/@ldf/server",
  "hasComponent": [
    {
      "@id": "ldfs:Datasource/Index",
      "@type": "ComponentConstructable",
      "rdfs:subClassOf": { "@id": "ldfs:Datasource" },
      "npm:requireElement": "datasources.IndexDatasource",
      "rdfs:comment": "An IndexDatasource is a datasource that lists other data sources",
      "hasParameter": [
        {
          "@id": "ldfs:Datasource/Index/datasource",
          "rdfs:subClassOf": {
            "@type": "owl:Restriction",
            "owl:onProperty": { "@id": "ldfs:datasource" },
            "owl:allValuesFrom": { "@id": "ldfs:Server/Qpf" }
          }
        }
      ],
      "constructorMapping": {
        "@list": [
          {
            "@type": "Object",
            "rdfs:subClassOf": { "@id": "ldfs:Datasource/ConstructorMappingObject" },
            "hasField": [
              {
                "@id": "ldfs:datasourcesFieldMapping"
              }
            ]
          }
        ]
      }
    }
  ]
}
