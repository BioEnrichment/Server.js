{
  "@context": {
    "@vocab": "http://linkedsoftwaredependencies.org/vocabulary/components#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "npmd": "http://example.org/npmdataset/",
    "npm": "http://example.org/npm#",
    "ldfs": "npmd:bundles/npm/ldf-server/",
    "rdfa": "http://www.w3.org/ns/rdfa#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "value": { "@id": "rdf:value", "@type": "@id"}
  },
  "@id": "npmd:bundles/npm/@ldf/server",
  "hasComponent": [
    {
      "@id": "ldfs:Datasource",
      "@type": "lsdc:ComponentConstructableAbstract",
      "hasParameter": [
        {
          "@id": "rdfs:label",
          "rdfs:comment": "The datasource name",
          "rdfs:range": "xsd:string"
        },
        {
          "@id": "rdfs:comment",
          "rdfs:comment": "The datasource description",
          "rdfs:range": "xsd:string",
          "parameterUnique": true
        },
        {
          "@id": "ldfs:Datasource/path",
          "rdfs:comment": "The relative path to the datasource from the baseURL",
          "rdfs:range": "xsd:string",
          "parameterUnique": true
        },
        {
          "@id": "ldfs:Datasource/enabled",
          "rdfs:comment": "If the datasource is enabled, by default true",
          "rdfs:range": "xsd:boolean",
          "parameterUnique": true
        },
        {
          "@id": "ldfs:Datasource/hidden",
          "rdfs:comment": "If the datasource must be hidden from the index, by default false",
          "rdfs:range": "xsd:boolean",
          "parameterUnique": true
        },
        {
          "@id": "ldfs:Datasource/urlData",
          "rdfs:subClassOf": {
            "@type": "owl:Restriction",
            "owl:onProperty": { "@id": "ldfs:urlData" },
            "owl:allValuesFrom": { "@id": "ldfs:Server/Qpf" }
          }
        }
      ],
      "constructorMapping": {
        "@list": [
          {
            "@id": "ldfs:Datasource/ConstructorMappingObject",
            "@type": "Object",
            "hasField": [
              {
                "rdfs:label": "id",
                "value": "rdf:subject"
              },
              {
                "rdfs:label": "title",
                "value": "rdfs:label"
              },
              {
                "rdfs:label": "description",
                "value": "rdfs:comment"
              },
              {
                "rdfs:label": "path",
                "value": "ldfs:Datasource/path"
              },
              {
                "rdfs:label": "enabled",
                "value": "ldfs:Datasource/enabled"
              },
              {
                "rdfs:label": "hide",
                "value": "ldfs:Datasource/hidden"
              },
              {
                "@id": "ldfs:urlDataFieldMapping"
              }
            ]
          }
        ]
      }
    }
  ]
}
