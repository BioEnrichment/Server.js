{
  "@context": "http://linkedsoftwaredependencies.org/contexts/ldf-server.jsonld",
  "@id": "npmd:ldf-server",
  "components": [
    {
      "@id": "ldfs:Datasource",
      "@type": "AbstractClass",
      "parameters": [
        {
          "@id": "ldfs:Datasource/title",
          "comment": "The datasource name",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/description",
          "comment": "The datasource description",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/path",
          "comment": "The relative path to the datasource from the baseURL",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/enabled",
          "comment": "If the datasource is enabled, by default true",
          "range": "xsd:boolean",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/hidden",
          "comment": "If the datasource must be hidden from the index, by default false",
          "range": "xsd:boolean",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/graph",
          "comment": "The default graph of the datasource",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/urlData",
          "inheritValues": {
            "@type": "InheritanceValue",
            "onParameter": "ldfs:urlData",
            "from": "ldfs:Server/Qpf"
          }
        }
      ],
      "constructorArguments": [
        {
          "@id": "ldfs:Datasource/constructorArgumentsObject",
          "fields": [
            {
              "keyRaw": "id",
              "value": "rdf:subject"
            },
            {
              "keyRaw": "title",
              "value": "ldfs:Datasource/title"
            },
            {
              "keyRaw": "description",
              "value": "ldfs:Datasource/description"
            },
            {
              "keyRaw": "path",
              "value": "ldfs:Datasource/path"
            },
            {
              "keyRaw": "enabled",
              "value": "ldfs:Datasource/enabled"
            },
            {
              "keyRaw": "hide",
              "value": "ldfs:Datasource/hidden"
            },
            {
              "keyRaw": "graph",
              "value": "ldfs:Datasource/graph"
            },
            {
              "@id": "ldfs:Server/Qpf/ObjectMappings/urlData"
            }
          ]
        }
      ]
    }
  ]
}
