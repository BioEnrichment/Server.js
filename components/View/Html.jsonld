      "extends": "ldfs:View",
      "comment": "HtmlView is a base class for views that generate HTML responses",
      "parameters": [
        {
          "@id": "ldfs:View/Html/cache",
          "comment": "If views should be cached",
          "range": "xsd:boolean",
          "parameterUnique": true
        },
        {
          "@id": "ldfs:View/Html/n3Util",
          "comment": "The N3Util class",
          "parameterUnique": true
        },
        {
          "@id": "ldfs:View/Html/urlData",
          "inheritValues": {
            "@type": "InheritanceValue",
