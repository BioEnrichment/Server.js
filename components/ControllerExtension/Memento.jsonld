      },
      "parameterUnique": {
        "@id": "lsdc:parameterUnique"
      },
      "hasDefaultValue": {
        "@id": "lsdc:hasDefaultValue"
      },
      "hasFixedValue": {
        "@id": "lsdc:hasFixedValue"
