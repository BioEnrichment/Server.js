  "@id": "npmd:@ldf/server",
  "components": [
    {
      "@id": "ldfs:ControllerExtension/Memento",
      "@type": "ComponentConstructable",
      "extends": "ldfs:ControllerExtension",
      "requireElement": "controllers.MementoControllerExtension",
      "comment": "A MementoControllerExtension extends Triple Pattern Fragments responses with Memento headers",
      "parameters": [
        {
          "@id": "ldfs:mementoTimegatePath",
          "inheritValues": {
            "@type": "InheritanceValue",
            "onParameter": "ldfs:timegatePath",
            "from": "ldfs:Controller/Timegate"
          }
        },
        {
          "@id": "ldfs:mementoMemento",
          "inheritValues": {
            "@type": "InheritanceValue",
            "onParameter": "ldfs:memento",
            "from": "ldfs:Controller/Timegate"
          }
        }
      ],
      "constructorMapping": [
        {
          "fields": [
            {
              "@id": "ldfs:timeGatesField"
            },
            {
              "@id": "ldfs:routersField"
            },
            {
              "@id": "ldfs:datasourcesFieldMapping"
            }
          ]
        }
      ]
    }
