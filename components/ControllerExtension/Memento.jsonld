        "@id": "owl:imports"
      },
      "InheritanceValue": {
        "@id": "owl:Restriction"
      },
      "inheritValues": {
        "@id": "rdfs:subClassOf",
        "@type": "@id"
      },
      "onParameter": {
        "@id": "owl:onProperty",
        "@type": "@id"
      },
      "from": {
        "@id": "owl:allValuesFrom",
        "@type": "@id"
      },

      "npm": "http://example.org/npm#",
      "requireName": {
        "@id": "npm:requireName"
      },
      "requireElement": {
        "@id": "npm:requireElement"
